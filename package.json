{
  "name": "sts-stats-analyzer",
  "productName": "StS Stats Analyzer",
  "private": true,
  "version": "1.2.3",
  "main": "./dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:typecheck": "tsc -b --noEmit && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:electron": "tsc -p electron/tsconfig.json",
    "watch:electron": "tsc -w -p electron/tsconfig.json",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:dev": "concurrently \"yarn dev\" \"yarn watch:electron\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron --name='StS Stats Analyzer' .\"",
    "clean": "rimraf dist dist-electron",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "buildL:mdc": "ts-node -P rules/tsconfig.json rules/build-mdc.ts",
    "watchL:mdc": "ts-node -P rules/tsconfig.json rules/watch-mdc.ts",
    "build:app": "vite build && electron-builder build --mac --win -c.extraMetadata.main=dist-electron/main.js",
    "build:win": "yarn build:electron && vite build && electron-builder build --win --config electron-builder.json --publish never",
    "build:mac": "yarn build:electron && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false vite build && electron-builder build --mac --config electron-builder.json --publish never",
    "build:all": "yarn clean && yarn build:electron && vite build && electron-builder build --mac --win --config electron-builder.json --publish never",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@reduxjs/toolkit": "^2.11.2",
    "chart.js": "^4.5.1",
    "chokidar": "^4.0.3",
    "daisyui": "^5.5.18",
    "date-fns": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-store": "8.1.0",
    "electron-updater": "^6.8.3",
    "framer-motion": "^12.34.2",
    "i18next": "^24.2.3",
    "lru-cache": "^11.2.6",
    "path-browserify": "^1.0.1",
    "react": "^19.2.4",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.4",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.12.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@electron/rebuild": "^3.7.1",
    "@eslint/js": "^9.22.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/postcss": "^4.2.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/chart.js": "^4.0.1",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^35.7.5",
    "electron-builder": "^25.1.8",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "glob": "^11.1.0",
    "globals": "^16.0.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^14.0.0",
    "rimraf": "^6.1.3",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^6.4.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^9.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moritaniantech/slay-the-spire-stats-analyzer.git"
  },
  "author": "moritaniantech <mr.de.cube@gmail.com>",
  "license": "MIT",
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "bugs": {
    "url": "https://github.com/moritaniantech/slay-the-spire-stats-analyzer/issues"
  },
  "homepage": "https://github.com/moritaniantech/slay-the-spire-stats-analyzer#readme",
  "keywords": [],
  "resolutions": {
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.23",
    "tar-fs": "^2.1.4",
    "tmp": "^0.2.4",
    "tar": "^7.5.8",
    "glob": ">=10.5.0",
    "minimatch": ">=10.0.0"
  }
}
