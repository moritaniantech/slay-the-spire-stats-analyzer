{
  "name": "sts-stats-analyzer",
  "productName": "StS Stats Analyzer",
  "private": true,
  "version": "1.0.0",
  "type": "commonjs",
  "main": "./dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b --noEmit && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:electron": "tsc -p electron/tsconfig.json",
    "watch:electron": "tsc -w -p electron/tsconfig.json",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:dev": "concurrently \"yarn dev\" \"yarn watch:electron\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron --name='StS Stats Analyzer' .\"",
    "clean": "rimraf dist dist-electron",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "buildL:mdc": "ts-node -P rules/tsconfig.json rules/build-mdc.ts",
    "watchL:mdc": "ts-node -P rules/tsconfig.json rules/watch-mdc.ts",
    "build:app": "vite build && electron-builder build --mac --win -c.extraMetadata.main=dist-electron/main.js",
    "build:win": "vite build && electron-builder build --win -c.extraMetadata.main=dist-electron/main.js",
    "build:mac": "vite build && electron-builder build --mac -c.extraMetadata.main=dist-electron/main.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@eslint/config-array": "^0.19.2",
    "@eslint/object-schema": "^2.1.6",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@npmcli/arborist": "^9.0.1",
    "@npmcli/fs": "^4.0.0",
    "@npmcli/package-json": "^6.1.1",
    "@reduxjs/toolkit": "^2.6.1",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.8",
    "crypto-browserify": "^3.12.1",
    "daisyui": "^5.0.6",
    "date-fns": "^4.1.0",
    "electron-is-dev": "^3.0.1",
    "framer-motion": "^12.5.0",
    "i18next": "^24.2.3",
    "lru-cache": "^11.0.2",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.4.0",
    "react-switch": "^7.1.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "stream-browserify": "^3.0.0",
    "typeorm": "^0.3.21",
    "util": "^0.12.5",
    "vite-plugin-node-polyfills": "^0.23.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@eslint/js": "^9.22.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/postcss": "^4.0.14",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/chart.js": "^2.9.41",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.19",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.0.3",
    "electron-builder": "^25.1.8",
    "electron-log": "^5.3.2",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "glob": "^11.0.1",
    "globals": "^16.0.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.2",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^8.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moritaniantech/slay-the-spire-stats-analyzer.git"
  },
  "author": "moritaniantech <mr.de.cube@gmail.com>",
  "license": "MIT",
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "bugs": {
    "url": "https://github.com/moritaniantech/slay-the-spire-stats-analyzer/issues"
  },
  "homepage": "https://github.com/moritaniantech/slay-the-spire-stats-analyzer#readme",
  "keywords": [],
  "build": {
    "appId": "com.momomo.sts-stats-analyzer",
    "productName": "StS Stats Analyzer",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icons/icon.ico"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": ["arm64", "x64"]
        }
      ],
      "icon": "public/icons/icon.png",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "entitlements": null,
      "entitlementsInherit": null,
      "darkModeSupport": true,
      "identity": null,
      "extendInfo": {
        "NSAppleEventsUsageDescription": "StS Stats Analyzerはゲームのプレイデータを分析するためにファイルアクセスが必要です。",
        "NSMicrophoneUsageDescription": "このアプリはマイクを使用しません。",
        "NSCameraUsageDescription": "このアプリはカメラを使用しません。",
        "NSLocationUsageDescription": "このアプリは位置情報を使用しません。"
      }
    },
    "dmg": {
      "sign": false,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "width": 540,
        "height": 380
      }
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "StS Stats Analyzer",
      "installerIcon": "public/icons/icon.ico",
      "uninstallerIcon": "public/icons/icon.ico"
    },
    "publish": {
      "provider": "github",
      "owner": "moritaniantech",
      "repo": "slay-the-spire-stats-analyzer",
      "private": false,
      "releaseType": "release",
      "updaterCacheDirName": "sts-stats-analyzer-updater"
    }
  }
}
